	Date-Modified = {2010-06-08 22:28:29 -0600},
	Pages = {852--859},
	Title = {{REGO}: Rank-based Estimation of {R}\'enyi Information using Euclidean Graph Optimization},
	Volume = {9},
	Year = {2010},
	Bdsk-Url-1 = {http://www.sztaki.hu/~szcsaba/papers/information_AIstat_v4.pdf}}

